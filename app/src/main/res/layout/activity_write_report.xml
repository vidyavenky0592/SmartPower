<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             tools:context=".WriteReport">

    <TextView
            android:id="@+id/emailtext"
            android:layout_width="72dp"
            android:layout_height="38dp"
            android:text="Email:"
            android:textSize="24sp"
            tools:layout_editor_absoluteX="66dp"
            tools:layout_editor_absoluteY="141dp" android:textColor="@android:color/black" android:textStyle="bold"
            tools:ignore="MissingConstraints" android:layout_alignParentTop="true" android:layout_marginTop="143dp"
            android:layout_alignStart="@+id/typeofItext" android:layout_marginStart="-25dp"/>

    <TextView
            android:id="@+id/nametext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name:"
            android:textSize="24sp"
            tools:layout_editor_absoluteX="62dp"
            tools:layout_editor_absoluteY="86dp" android:textStyle="bold" android:textColor="@android:color/black"
            tools:ignore="MissingConstraints" android:layout_alignParentStart="true" android:layout_marginStart="38dp"
            android:layout_alignParentTop="true" android:layout_marginTop="87dp"/>

    <TextView
            android:id="@+id/typeofItext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="What type of incident is it?"
            android:textSize="24sp"
            tools:layout_editor_absoluteY="285dp" android:textColor="@android:color/black" android:textStyle="bold"
            android:layout_marginEnd="61dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="64dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.623"
            tools:ignore="MissingConstraints" android:layout_alignParentBottom="true"
            android:layout_marginBottom="292dp" android:layout_alignParentEnd="true"
            android:layout_alignParentStart="true"/>

    <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="164dp"
            tools:layout_editor_absoluteY="128dp" tools:ignore="MissingConstraints"/>

    <Button
            android:id="@+id/report_send"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Send"
            tools:layout_editor_absoluteX="164dp"
            tools:layout_editor_absoluteY="409dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Widget.Button.Colored" android:textSize="18sp"
            android:textColor="@android:color/white" tools:ignore="MissingConstraints"
            style="@style/Widget.AppCompat.Button.Colored" android:background="@color/design_default_color_primary"
            android:layout_alignParentStart="true" android:layout_marginStart="166dp"
            android:layout_alignParentTop="true" android:layout_marginTop="385dp"/>
    <TextView
            android:text="Report An Incident"
            android:layout_width="264dp"
            android:layout_height="43dp" tools:layout_editor_absoluteY="16dp"
            android:id="@+id/textView"
            android:textColor="@color/design_default_color_primary" android:textSize="30sp"
            android:layout_marginEnd="72dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="75dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.504"
            tools:ignore="MissingConstraints" android:layout_marginTop="23dp" android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true" android:layout_alignParentStart="true"/>
    <EditText
            android:layout_width="333dp"
            android:layout_height="30dp"
            android:inputType="textMultiLine"
            android:ems="10"
            tools:layout_editor_absoluteY="344dp" android:id="@+id/editText2" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="37dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.87"
            tools:ignore="MissingConstraints" android:layout_marginTop="328dp" android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"/>
    <TextView
            android:text="Location:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="200dp"
            tools:layout_editor_absoluteX="38dp" android:id="@+id/textView2" android:textSize="24sp"
            android:textStyle="bold" android:textColor="@android:color/black" tools:ignore="MissingConstraints"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="367dp" android:layout_alignEnd="@+id/emailtext"
            android:layout_marginEnd="-6dp"/>
    <EditText
            android:layout_width="244dp"
            android:layout_height="31dp"
            android:inputType="textPostalAddress"
            android:ems="10"
            tools:layout_editor_absoluteY="200dp" tools:layout_editor_absoluteX="164dp" android:id="@+id/editText4"
            tools:ignore="MissingConstraints" android:layout_toEndOf="@+id/textView2"
            android:layout_alignParentTop="true" android:layout_marginTop="204dp" android:layout_marginStart="27dp"/>
    <ImageView
            android:id="@+id/imageView"
            android:layout_width="171dp"
            android:layout_height="62dp"
            android:layout_marginTop="508dp"
            android:layout_marginEnd="111dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/dominion_energy" android:layout_alignParentEnd="true"
            android:layout_alignParentStart="true" android:layout_marginStart="129dp"/>
    <EditText
            android:layout_width="245dp"
            android:layout_height="35dp"
            android:inputType="textMultiLine"
            android:ems="10"
            tools:layout_editor_absoluteY="344dp" tools:layout_editor_absoluteX="62dp" android:id="@+id/editText10"
            android:layout_alignParentEnd="true" android:layout_alignParentTop="true" android:layout_marginTop="88dp"
            android:layout_marginEnd="32dp"/>
    <EditText
            android:layout_width="245dp"
            android:layout_height="35dp"
            android:inputType="textMultiLine"
            android:ems="10"
            tools:layout_editor_absoluteY="344dp" tools:layout_editor_absoluteX="62dp" android:id="@+id/editText"
            android:layout_alignParentEnd="true" android:layout_alignParentTop="true" android:layout_marginTop="142dp"
            android:layout_marginEnd="29dp"/>
    <SearchView
            android:layout_width="30dp"
            android:layout_height="36dp" android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true" android:layout_marginTop="203dp" android:layout_marginEnd="3dp"/>


</RelativeLayout>
